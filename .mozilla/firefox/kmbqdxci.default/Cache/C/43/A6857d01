    window.debugEventTracker = false; // Override in console for testing

    // Bind element event to Google Analytics tracker
    function bindEventTracker(selector, eventType, message) {
      if ($(selector).length < 1 && window.console) {
        console.warn('bindEventTracker - Could not find: ' + selector);
      }
      $(selector).on(eventType, function(event) {
        if (window.debugEventTracker) {
          alert('Track Event: ' + message);
          event.preventDefault();
        } else {
          window._gaq && _gaq.push(['_trackEvent', message]);
        }
      });
    }

    $(function() {
      bindEventTracker('.widget_social_links .rss',     'click',        'Follow RSS');
      bindEventTracker('.widget_social_links .rss',     'contextmenu',  'Right-click RSS');
      bindEventTracker('.widget_social_links .twitter', 'click',        'Follow Twitter');
    });
  </script>
